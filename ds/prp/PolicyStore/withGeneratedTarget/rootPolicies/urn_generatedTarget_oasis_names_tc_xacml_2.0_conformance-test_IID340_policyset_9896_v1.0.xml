    <Policy PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IID340:policy1" Version="1.0" RuleCombiningAlgId="urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:permit-overrides">
