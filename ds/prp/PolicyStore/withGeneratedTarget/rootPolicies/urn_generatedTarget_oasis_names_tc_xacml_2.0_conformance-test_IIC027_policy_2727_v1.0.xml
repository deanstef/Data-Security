    <Rule RuleId="urn:oasis:names:tc:xacml:2.0:conformance-test:IIC027:rule" Effect="Permit">
        <Description>
            A subject who is at least 5 years different in age
            from Bart Simpson may read Bart Simpson's medical
            record.
        </Description>
        <Condition>
            <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:double-greater-than-or-equal">
                <Apply FunctionId="urn:oasis:names:tc:xacml:1.0:function:double-abs">
