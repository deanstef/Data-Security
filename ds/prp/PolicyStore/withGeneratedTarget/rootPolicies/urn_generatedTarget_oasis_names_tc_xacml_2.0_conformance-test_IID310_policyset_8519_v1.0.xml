    <Policy PolicyId="urn:oasis:names:tc:xacml:2.0:conformance-test:IID310:policy2" Version="1.0" RuleCombiningAlgId="urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:ordered-deny-overrides">
